    "husky": "8.0.3",
    "jest": "29.6.4",
    "jest-html-reporter": "3.10.2",
    "jest-when": "3.6.0",
    "jotai": "2.4.0",
    "jsdoc": "4.0.2",
    "jsdoc-tsimport-plugin": "1.0.5",
    "pactum": "3.5.1",
    "pactum-matchers": "1.1.6",
    "prettier": "3.0.1",
    "react": "18.2.0",
    "react-cookie": "6.1.1",
    "react-dom": "18.2.0",
    "react-refresh": "0.14.0",
    "react-router-dom": "6.15.0",
    "rimraf": "5.0.1",
    "sass": "1.65.1",
    "ts-node": "10.9.1",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "wireit": "0.13.0"
  }
}
