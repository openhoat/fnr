    "@fastify/swagger-ui": "1.9.3",
    "@fastify/vite": "4.0.0",
    "@hapi/boom": "10.0.1",
    "change-case": "4.1.2",
    "common-tags": "1.8.2",
    "dotenv": "16.3.1",
    "fastify": "4.21.0",
    "fastify-plugin": "4.5.1",
    "find-root": "1.1.0",
    "http-status-codes": "2.2.0",
